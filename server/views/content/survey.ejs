<h1><%= title %></h1>
<p>Welcome to <%= title %> surveys</p>

<div class="row col-5">
    <h4 class="fw-bold text-center mt-3"></h4>
    <form class=" bg-white px-4" action="">
      <p class="fw-bold">Test question: How satisfied are you with out product</p>
      <div class="form-check mb-2">
        <input class="form-check-input" type="radio" name="Form" id="radio1" />
        <label class="form-check-label" for="radioExample1">
          Great
        </label>
      </div>
      <div class="form-check mb-2">
        <input class="form-check-input" type="radio" name="Form" id="radio2" />
        <label class="form-check-label" for="radioExample2">
          Medicore
        </label>
      </div>
      <div class="form-check mb-2">
        <input class="form-check-input" type="radio" name="Form" id="radio3" />
        <label class="form-check-label" for="radioExample3">
          Bad
        </label>
      </div>
    </form>
    <div class="text-end">
        <button type="submit" class="btn btn-primary">Submit</button>
    </div>
</div>
